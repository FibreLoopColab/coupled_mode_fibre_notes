@article{Longhi2015,
  title = {Non-{{Hermitian}} Transparency and One-Way Transport in Low-Dimensional Lattices by an Imaginary Gauge Field},
  author = {Longhi, Stefano and Gatti, Davide and Della Valle, Giuseppe},
  date = {2015-09-28},
  journaltitle = {Physical Review B},
  shortjournal = {Phys. Rev. B},
  volume = {92},
